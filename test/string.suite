describe 'A unicorn'

equals_self() {
        expect 'unicorn' to_equal 'unicorn'  
}

it 'should be equal to itself' equals_self

not_equals_pony() {
        ! expect 'unicorn' not to_equal 'pony'
}

it 'should not be equal to a pony' not_equals_pony

ends_with_corn() {
        expect 'unicorn' to_match '^.*corn$'
}

it 'should end with the word corn' ends_with_corn 
