describe 'A unicorn'

equals_self() {
  expect 'unicorn' to_equal 'unicorn'  
}

xit 'should be equal to itself' equals_self

not_equals_pony() {
  expect 'unicorn' not to_equal 'unicorn'
}

it 'should not be equal to a pony' not_equals_pony

ends_with_corn() {
  expect 'unicorn' to_end_with 'corn'
}

it 'should end with the word corn' ends_with_corn 
